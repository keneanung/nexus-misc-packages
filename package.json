{
  "name": "@keneanung/nexus-misc-packages",
  "version": "0.0.0",
  "description": "A collection of smaller packages for Nexus 3",
  "author": "keneanung <keneanung@gmail.com>",
  "license": "MIT",
  "repository": "git://github.com/keneanung/nexus-misc-packages.git",
  "main": "lib/index.js",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@keneanung/iron-realms-nexus-typings": "^0.3.0",
    "@keneanung/nexus-package-builder": "^1.2.0",
    "@stryker-mutator/core": "^6.2.3",
    "@stryker-mutator/jest-runner": "^6.2.3",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "acorn": "^8.8.0",
    "babel-jest": "^29.2.2",
    "babel-loader": "^9.0.0",
    "babel-plugin-react-native-web": "^0.19.1",
    "core-js": "^3.30.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsdoc": "^43.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.0.0",
    "jest": "^29.2.2",
    "metro-react-native-babel-preset": "^0.76.0",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.10",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^5.0.2",
    "webpack": "^5.74.0",
    "webpack-auto-inject-version-next": "^1.2.4",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "write-pkg": "^5.1.0"
  },
  "scripts": {
    "serve": "webpack serve --mode development",
    "build": "tsc",
    "webpack": "npm test && webpack build --mode development",
    "webpackProd": "npm test && webpack build --mode production",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --testLocationInResults --collect-coverage --config jestconfig.json",
    "watchTest": "jest --collect-coverage --config jestconfig.json --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "website": "cd website && npm run start",
    "setup": "node ./setup.mjs",
    "nxs": "npm run webpackProd && nexus-package-builder nxs/queueManager.yaml nxs/"
  },
  "files": [
    "lib/**/*",
    "!lib/**/__tests__/*",
    "!lib/webpack/**/*"
  ],
  "dependencies": {
  }
}
